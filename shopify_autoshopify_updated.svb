!#ClearCookies FUNCTION ClearCookies 

# Step 1: Fetch products.json
REQUEST GET "https://<site>/products.json" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/137.0.6367.207 Safari/537.36" 
  HEADER "Accept: application/json" 
  HEADER "Accept-Language: en-US,en;q=0.9" 

PARSE "<SOURCE>" LR "" "" -> VAR "products_raw" 

#variantid PARSE "<SOURCE>" LR "\"variant\":{\"id\":" "," CreateEmpty=FALSE -> VAR "variantid" 

#price1 PARSE "<SOURCE>" LR "\"price\":\"" "\"" CreateEmpty=FALSE -> VAR "price1" 

# Extract first available product variant ID and price
#variantid PARSE "<SOURCE>" LR "\"variants\":[{\"id\":" "," CreateEmpty=FALSE -> VAR "variantid" 

#price1 PARSE "<SOURCE>" LR "\"price\":\"" "\"" CreateEmpty=FALSE -> VAR "price1" 

# Step 2: Create cart via GraphQL API
#CART_CREATE REQUEST POST "https://<site>/api/unstable/graphql.json" 
  CONTENT "{\"query\":\"mutation CartCreate($input: CartInput!) { cartCreate(input: $input) { cart { checkoutUrl } } }\",\"variables\":{\"input\":{\"lines\":[{\"merchandiseId\":\"gid://shopify/ProductVariant/<variantid>\",\"quantity\":1,\"attributes\":[]}],\"discountCodes\":[]},\"country\":\"US\",\"language\":\"EN\"}}" 
  CONTENTTYPE "application/json" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/137.0.6367.207 Safari/537.36" 
  HEADER "Accept: application/json" 
  HEADER "Accept-Language: en-US,en;q=0.9" 

#checkout_url PARSE "<SOURCE>" LR "\"checkoutUrl\":\"" "\"" CreateEmpty=FALSE -> VAR "checkout_url" 

# Step 3: Get checkout page
#CHECKOUT REQUEST GET "<checkout_url>?auto_redirect=false" AutoRedirect=FALSE 
  
